dw          0,  1000,  3000,  5000,  7000, 9000, 13000, 14000
dw     -15000,-14000,-13000,-9000, -7000, -5000, -3000, -1000
dw          0,  1000,  3000,  5000,  7000, 9000, 13000, 14000
dw     -15000,-14000,-13000,-9000, -7000, -5000, -3000, -1000
dw          0,  1000,  3000,  5000,  7000, 9000, 13000, 14000
